{
  "name": "@eclesia/core-modules-pg",
  "version": "2.8.0",
  "description": "Eclesia postgres indexer core modules",
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit && tsdown",
    "postbuild": "copyfiles -u 1 \"src/**/*.sql\" dist",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest"
  },
  "files": [
    "dist/"
  ],
  "keywords": [],
  "contributors": [
    "Alex M.<alex.megalokonomos@tendermint.com>"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "@eclesia/basic-pg-indexer": "workspace:*",
    "@eclesia/indexer-engine": "workspace:*",
    "@eslint/js": "^9.35.0",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/json-bigint": "^1.0.4",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.15.5",
    "copyfiles": "^2.4.1",
    "eslint": "^9.35.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "pg": "^8.16.3",
    "tsdown": "^0.15.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@cosmjs/encoding": "^0.36.0",
    "@cosmjs/proto-signing": "^0.36.0",
    "@cosmjs/tendermint-rpc": "^0.36.0",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.3.1",
    "cosmjs-types": "^0.10.1",
    "json-with-bigint": "^3.4.4"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  }
}