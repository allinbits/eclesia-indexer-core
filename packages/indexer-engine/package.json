{
  "name": "@eclesia/indexer-engine",
  "version": "2.10.0",
  "description": "Core eclesia indexer engine",
  "scripts": {
    "build": "tsc --noEmit && tsdown",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.mjs"
  },
  "files": [
    "dist/",
    "scripts/",
    "templates"
  ],
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "keywords": [],
  "contributors": [
    "Alex M.<alex.megalokonomos@tendermint.com>"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/node": "^22.19.0",
    "@types/stream-chain": "^2.1.0",
    "@types/stream-json": "^1.7.8",
    "@types/uuid": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "tsdown": "^0.16.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@cosmjs/tendermint-rpc": "^0.37.0",
    "bech32": "^2.0.0",
    "cosmjs-types": "^0.10.1",
    "dayjs": "^1.11.19",
    "fastify": "^5.6.1",
    "prom-client": "^15.1.3",
    "stream-chain": "^3.4.0",
    "stream-json": "^1.9.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  }
}